# Model Storage Configuration
# Set to 'true' to use local storage (INSTANT - no S3 upload)
# Set to 'false' to use S3 with parallel uploads (faster than before)
USE_LOCAL_STORAGE=true

# S3 Configuration (only needed if USE_LOCAL_STORAGE=false)
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1
S3_BUCKET=your-bucket-name

# EC2 GPU Training Configuration (No Local Docker)
# Replace these with your actual EC2 instance details
EXISTING_GPU_INSTANCE_ID=i-0123456789abcdef0
EC2_KEY_NAME=your-ec2-keypair
EC2_SECURITY_GROUP_ID=sg-0123456789abcdef0
EC2_SUBNET_ID=subnet-0123456789abcdef0
GPU_INSTANCE_TYPE=g4dn.xlarge
SKIP_GPU_SETUP=true

# Database Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key